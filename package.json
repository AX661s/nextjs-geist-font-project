{
  "name": "phone-social-tracker",
  "version": "1.0.0",
  "description": "Phone & Social Media Tracker with User Authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "install-deps": "npm install",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "build:css": "tailwindcss -i ./src/styles/osint.css -o ./public/osint.css --watch",
    "build:css:prod": "tailwindcss -i ./src/styles/osint.css -o ./public/osint.css --minify"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.17.3",
    "helmet": "^8.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "node-fetch": "^2.6.9",
    "pino": "^8.16.0",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.1",
    "pino-pretty": "^10.2.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13"
  },
  "keywords": [
    "phone-tracker",
    "social-media",
    "authentication",
    "express",
    "nodejs"
