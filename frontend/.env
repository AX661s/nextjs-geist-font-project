# React App Configuration - 性能优化版
REACT_APP_BACKEND_URL=http://localhost:10010
REACT_APP_API_URL=http://localhost:10010/v2
REACT_APP_SERVER_ENDPOINT=http://localhost:10010
NODE_ENV=development
PORT=3000
# 性能优化设置
GENERATE_SOURCEMAP=false
NODE_OPTIONS=--openssl-legacy-provider --max-old-space-size=1024
FAST_REFRESH=true
DISABLE_ESLINT_PLUGIN=true
TSC_COMPILE_ON_ERROR=true
SKIP_PREFLIGHT_CHECK=true
# 禁用不必要的功能
DISABLE_NEW_JSX_TRANSFORM=false
INLINE_RUNTIME_CHUNK=false
